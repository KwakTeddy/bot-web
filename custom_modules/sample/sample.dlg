<재시> {start: 1}     // 시작
<이전> {up: 1}        // 상위 (대부분은 이전에 사용됨)
<뒤로> {back: 1}      // 뒤로 숫자 만큼
<다시> {repeat: 1}    // 재질문
<custom> function(dialog, context, print, callback) { print('직접 출력 및 custom 처리'); callback(true); }
  

